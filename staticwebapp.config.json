{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET",
                "POST",
                "DELETE",
                "PUT",
                "OPTIONS"
            ],
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "mimeTypes": {
        ".json": "application/json"
    }
}